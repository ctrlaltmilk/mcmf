{
  "multipart": [
    {
      "when": {
        "OR": [
          { "north": "none", "east": "none", "south": "none", "west": "none" },
          { "north": "side|up", "east": "none", "west": "none" },
          { "east": "none", "south": "side|up", "west": "none" }
        ]
      },
      "apply": { "model": "mcmf:block/cable_straight" }
    },
    {
      "when": {
        "OR": [
          { "north": "none", "east": "side|up", "south": "none" },
          { "north": "none", "south": "none", "west": "side|up" }
        ]
      },
      "apply": { "model": "mcmf:block/cable_straight", "y": 90 }
    },
    {
      "when": { "north": "side|up", "east": "side|up", "south": "none", "west": "none" },
      "apply": { "model": "mcmf:block/cable_bend_ne"}
    },
    {
      "when": { "north": "side|up", "east": "none", "south": "none", "west": "side|up" },
      "apply": { "model": "mcmf:block/cable_bend_nw" }
    },
    {
      "when": { "north": "none", "east": "side|up", "south": "side|up", "west": "none" },
      "apply": { "model": "mcmf:block/cable_bend_se"}
    },
    {
      "when": { "north": "none", "east": "none", "south": "side|up", "west": "side|up" },
      "apply": { "model": "mcmf:block/cable_bend_sw" }
    },
    {
      "when": {
        "OR": [
          { "north": "side|up", "east": "side|up", "south": "side|up", "west": "none" },
          { "north": "side|up", "east": "side|up", "south": "none", "west": "side|up" },
          { "north": "side|up", "east": "none", "south": "side|up", "west": "side|up" },
          { "north": "none", "east": "side|up", "south": "side|up", "west": "side|up" },
          { "north": "side|up", "east": "side|up", "south": "side|up", "west": "side|up" }
        ]
      },
      "apply": { "model": "mcmf:block/cable_junction"}
    },
    {
      "when": {
        "OR": [
          { "north": "side|up", "east": "side|up" },
          { "north": "side|up", "west": "side|up" }
        ]
      },
      "apply": { "model": "mcmf:block/cable_side0"}
    },
    {
      "when": {
        "OR": [
          { "north": "side|up", "east": "side|up" },
          { "south": "side|up", "east": "side|up" }
        ]
      },
      "apply": { "model": "mcmf:block/cable_side0", "y": 90 }
    },
    {
      "when": {
        "OR": [
          { "south": "side|up", "east": "side|up" },
          { "south": "side|up", "west": "side|up" }
        ]
      },
      "apply": { "model": "mcmf:block/cable_side1", "y": 180 }
    },
    {
      "when": {
        "OR": [
          { "north": "side|up", "west": "side|up" },
          { "south": "side|up", "west": "side|up" }
        ]
      },
      "apply": { "model": "mcmf:block/cable_side1", "y": 270 }
    },
    {
      "when": { "north": "up" },
      "apply": { "model": "mcmf:block/cable_wall0" }
    },
    {
      "when": { "east": "up" },
      "apply": { "model": "mcmf:block/cable_wall0", "y": 90 }
    },
    {
      "when": { "south": "up" },
      "apply": { "model": "mcmf:block/cable_wall1", "y": 180 }
    },
    {
      "when": { "west": "up" },
      "apply": { "model": "mcmf:block/cable_wall1", "y": 270 }
    }
  ]
}
